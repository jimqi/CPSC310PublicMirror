<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->
	<servlet>
		<servlet-name>loginService</servlet-name>
		<servlet-class>com.google.gwt.killers.server.LoginServiceImpl
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>parkService</servlet-name>
		<servlet-class>com.google.gwt.killers.server.ParkServiceImpl
		</servlet-class>
	</servlet>
	
		<servlet>
		<servlet-name>restaurantService</servlet-name>
		<servlet-class>com.google.gwt.killers.server.RestaurantServiceImpl
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>loginService</servlet-name>
		<url-pattern>/killersproject/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>parkService</servlet-name>
		<url-pattern>/killersproject/park</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>restaurantService</servlet-name>
		<url-pattern>/killersproject/restaurant</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>KillersProject.html</welcome-file>
	</welcome-file-list>

</web-app>
